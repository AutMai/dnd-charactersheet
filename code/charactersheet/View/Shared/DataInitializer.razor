@using Model.Entities
@typeparam TData

@if (Data is null) {
    <img src="816008710836715520.png"/>
}
else {
    if (Data is Character c) {
        Console.WriteLine("CHARACTER NAME:   " + c.Name);
    }
    @ChildComponent(Data)
}

@code {
    [Parameter]
    public RenderFragment<TData> ChildComponent { get; set; }

    [Parameter]
    public TData Data { get; set; }
}